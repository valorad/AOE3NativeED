<?xml version="1.0" encoding="utf-8"?>

<protounitsounddef>
	<protounit name="WCEAmiasShield">
		<soundtype name="Select">
			<soundset name="IroquoisMantletSelect"/>
		</soundtype>
		<soundtype name="Grunt">
			<soundset name="GenericMaleGrunt"/>
		</soundtype>
		<soundtype name="Death">
			<soundset name="GenericMaleDeath"/>
		</soundtype>
		<soundtype name="Creation">
			<soundset name="NativeBirth"/>
		</soundtype>
		<soundtype name="Acknowledge">
			<targetlogic>
				<choice name="default">
					<soundset name="IroquoisMantletAcknowledge"/>
				</choice>
				<choice name="enemy">
					<soundset name="IroquoisMantletAttack"/>
				</choice>
			</targetlogic>
		</soundtype>
	</protounit>
</protounitsounddef>